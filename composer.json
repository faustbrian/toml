{
    "name": "cline/toml",
    "description": "A PHP parser for TOML compatible with specification 0.4.0",
    "license": "MIT",
    "keywords": [
        "php",
        "toml",
        "parser",
        "configuration"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.5.0"
    },
    "require-dev": {
        "cline/coding-standard": "^1.6.3",
        "orchestra/testbench": "^10.8",
        "symfony/var-dumper": "^7.4.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\Toml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/ecs check --fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/ecs check",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --parallel --min=80 --type-coverage",
        "test:types": "phpstan",
        "test:unit": "pest --parallel --min=80 --coverage"
    }
}
